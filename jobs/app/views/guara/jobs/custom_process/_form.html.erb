<%= semantic_form_for @custom_process, 
    :url => custom_process_path(@custom_process), 
    :html => {:class => "well"}  do |f| %>
  
  <fieldset>
    <legend>
      
    </legend>

    <div class="control-group">   
      <%= f.input :name, :class => "span6" %>
      <%= f.input :enabled, :class => "span6" %>
      <%= f.submit class: "btn btn-primary" %>
    </div>

  </fieldset>
<% end %>