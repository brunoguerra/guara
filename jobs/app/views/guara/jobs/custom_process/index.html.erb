<section id="jobs">
	<div>
		<navbar>
			<%= render :partial => "guara/jobs/jobs/navbar", 
			:locals => { active: :custom_process } %>
		</navbar>
		
		<div>
		<div class="row-fluid">
			<ul class="thumbnails">
			<li class="span8">
				<div class="thumbnail">
				<div class="caption">
					<div class="page-header">
						<%= t("search.results")%>
					</div>

					<table class="table table-bordered table-striped">
						<thead>
							<tr>
								<th class="span1"><%= t("custom_process.id") %></th>
								<th><%= t("jobs.custom_process.name") %></th>
								<th class="span1"><%= t("helpers.forms.actions") %></th>
							</tr>
						</thead>
						<tbody>
							<% @custom_process.each do |p| %>
							<tr class="<%= p.enabled == false ? 'warning' : '' %>">
								<td><%= p.id %></td>
								<td><%= link_to p.name, custom_proces_path(p)  %></td>
								<td>

									<%= index_btn_view custom_proces_path(p), "Exibir Processo" %>
								</td>
							</tr>
							<% end %>

						</tbody>
					</table>

					<%= link_to t("jobs.custom_process.new"), new_custom_proces_path , :class=> "btn btn-primary" %>

				</div>
				</div>
			</li>

			<li class="span4">
				<div class="thumbnail">
					<div class="caption">

					</div>
				</div>
			</li>

			</ul>
		</div>	
		</div>
		
	</div>

</div>


