<section id="jobs">

	<div>
		<navbar>
			<%= render :partial => "guara/jobs/jobs/navbar", 
      :locals => { active: :custom_process } %>
		</navbar>
		
		<div>
			<div class="row-fluid">
		        <ul class="thumbnails">
		          <li class="span8">
		            <div class="thumbnail">
		              <div class="caption">
		                <div class="page-header">
		                  <%= t("search.results")%>
		                </div>

		                <table class="table table-bordered table-striped">
						    <thead>
						      <tr>
						      	<th><%= t("custom_process.id") %></th>
						        <th><%= t("custom_process.name") %></th>
						        <th><%= t("helpers.forms.actions") %></th>
						      </tr>
						    </thead>
						    <tbody>
							<% @custom_process.each do |p| %>
						      <tr class="<%= p.enabled == false ? 'warning' : '' %>">
						        <td><%= p.id %></td>
						        <td><%= link_to p.name, custom_proces_path(p)  %></td>
						        <td>
						        	<%= link_to t("helpers.forms.show"), custom_proces_path(p), :class => "btn" %>
						        </td>
						      </tr>
						    <% end %>

						    </tbody>
						</table>

		                <%= link_to t("helpers.forms.new"), new_custom_proces_path , :class=> "btn" %>

		              </div>
		            </div>
		          </li>

		          <li class="span4">
		            <div class="thumbnail">
		              <div class="caption">
		                
		              </div>
		            </div>
		          </li>

		        </ul>
		    </div>	
		</div>
		
	</div>
  
</div>


