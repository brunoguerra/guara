<section id="vacancies_resume">
  <div>
    <navbar>
      <%= render :partial => "guara/jobs/jobs/navbar", :locals => { active: :vacancy_reports } %>
    </navbar>

    <div>
      <div class="row-fluid">
        <ul class="thumbnails">

          <li class="span8">
            <div class="thumbnail">
              <div class="caption">
                <div class="page-header">
                  <% @type_translated = Guara::Jobs::VacancyResume.type_translated() %>
                  <h2><%=@type_translated[params[:resume_type].to_i] %></h2>
                </div>

                <%= render partial: 'widget_vacancy_resume_chart' %>
                <b><%=@type_translated[params[:resume_type].to_i] %>: <%= @vacancy %></b>

              </div>
            </div>
          </li>

          <li class="span4">
            <div class="thumbnail">
              <div class="caption">
                <%= render :partial => 'guara/jobs/vacancies/form_search', locals: { url_search: vacancy_resume_index_path } %>
              </div>
            </div>
          </li>

        </ul>
      </div>  
    </div>
    
  </div>

</section>
