<navbar>
			<%= render :partial => "guara/jobs/jobs/navbar", 
      :locals => { active: :announcement_of_vacancies } %>
		</navbar>
		
<section class="show">
	<div class="">
		<ul class="thumbnails">
			<li class="span12">
			    <div class="thumbnail">
					<% if (@process_instance.step.level>0) %>
						<%= render :partial => 'details_initial_stage', :locals=> {:grouped_column_attrs_step_init=> @grouped_column_attrs_step_init, :no_button=> true } %>
					<% end %>
				</div>
			</li>
	  		<li class="span12">
			    <div class="thumbnail process_instance form">
			    	<%= render :partial => 'form' %>
				</div>
			</li>
		</ul>
	</div>	

	<%= link_to t('helpers.forms.back'), process_instance_path(@process_instance), :class => "btn" %>
</section>
