<section id="stock">

	<div id="dashboard">
			<navbar>
				<%= render :partial => "header" %>
			</navbar>

		<div class="row">
			<div class="span12">
				<fieldset>
				<section id="index">
				  <div class="page-header">
					<%= t("search.results")%>
				  </div>

				  <table class="table table-bordered table-striped">
				    <thead>
				      <tr>
				        <th><%= t("customer_pfs.company") %></th>
				        <th><%= t("orders.date_init.title") %></th>
				      </tr>
				    </thead>
				    <tbody>
				   
				   <% @order.each do |order| %>
					  <tr>
					    <td><%= order.person.name %></td>
					    <td><%= order.date_init %></td>
					  </tr>
					<% end %>

				    </tbody>
				  </table>

				  <%= link_to t('helpers.forms.new'), new_orders_in_path, :class=> "btn" %>
				</section>
				
			</fieldset>
		</div>
</div>